{
    "version": 2,
    "builds": [
      {
        "src": "server/**/*.mjs",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/$1"
      }
    ]
  }
  